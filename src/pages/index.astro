---
import { getCollection } from 'astro:content';
const folders = await getCollection('folders');
const photos = await getCollection('photos');
// console.log({folders, photos})
---
<h1>Folders</h1>
<ul>
  {folders.map(folder => (
    <li>{folder.id}</li>
  ))}
</ul>
<h1>Photos</h1>
<ul>
  {photos.map(photo => (
    <li>{photo.id}</li>
  ))}
</ul>